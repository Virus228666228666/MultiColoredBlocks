{
    "multipart": [
        {   "apply": { "model": "multicoloredblocks:redironbarspostends" }},
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "multicoloredblocks:redironbarspost" }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "multicoloredblocks:redironbarscap" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "multicoloredblocks:redironbarscap", "y": 90 }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "multicoloredblocks:redironbarscapalt" }
        },
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "multicoloredblocks:redironbarscapalt", "y": 90 }
        },
        {   "when": { "north": true },
            "apply": { "model": "multicoloredblocks:redironbarsside" }
        },
        {   "when": { "east": true },
            "apply": { "model": "multicoloredblocks:redironbarsside", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "multicoloredblocks:redironbarssidealt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "multicoloredblocks:redironbarssidealt", "y": 90 }
        }
    ]
}
