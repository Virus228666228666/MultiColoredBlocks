{
  "multipart": [
    {   "apply": { "model": "multicoloredblocks:block/bluefencepost" }},
    {   "when": { "north": "true" },
      "apply": { "model": "multicoloredblocks:block/bluefenceside", "uvlock": true }
    },
    {   "when": { "east": "true" },
      "apply": { "model": "multicoloredblocks:block/bluefenceside", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "true" },
      "apply": { "model": "multicoloredblocks:block/bluefenceside", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "true" },
      "apply": { "model": "multicoloredblocks:block/bluefenceside", "y": 270, "uvlock": true }
    }
  ]
}
