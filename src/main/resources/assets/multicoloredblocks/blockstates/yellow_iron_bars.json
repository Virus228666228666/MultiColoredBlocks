{
    "multipart": [
        {   "apply": { "model": "multicoloredblocks:block/yellowironbarspostends" }},
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "multicoloredblocks:block/yellowironbarspost" }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "multicoloredblocks:block/yellowironbarscap" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "multicoloredblocks:block/yellowironbarscap", "y": 90 }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "multicoloredblocks:block/yellowironbarscapalt" }
        },
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "multicoloredblocks:block/yellowironbarscapalt", "y": 90 }
        },
        {   "when": { "north": true },
            "apply": { "model": "multicoloredblocks:block/yellowironbarsside" }
        },
        {   "when": { "east": true },
            "apply": { "model": "multicoloredblocks:block/yellowironbarsside", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "multicoloredblocks:block/yellowironbarssidealt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "multicoloredblocks:block/yellowironbarssidealt", "y": 90 }
        }
    ]
}
