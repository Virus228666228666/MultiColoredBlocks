{
  "multipart": [
    {   "apply": { "model": "multicoloredblocks:block/blackwhitefencepost" }},
    {   "when": { "north": "true" },
      "apply": { "model": "multicoloredblocks:block/blackwhitefenceside", "uvlock": true }
    },
    {   "when": { "east": "true" },
      "apply": { "model": "multicoloredblocks:block/blackwhitefenceside", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "true" },
      "apply": { "model": "multicoloredblocks:block/blackwhitefenceside", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "true" },
      "apply": { "model": "multicoloredblocks:block/blackwhitefenceside", "y": 270, "uvlock": true }
    }
  ]
}
